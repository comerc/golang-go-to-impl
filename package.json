{
  "name": "golang-go-to-impl",
  "displayName": "Go to Implementation (Golang)",
  "icon": "media/icon.png",
  "description": "Excludes specified files when using Go to Implementation in VS Code for Go projects.",
  "version": "1.0.0",
  "publisher": "comerc",
  "engines": {
    "vscode": "^1.60.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/comerc/golang-go-to-impl.git"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "go",
    "golang",
    "mock",
    "implementation",
    "navigation",
    "gopls",
    "testing",
    "interface"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "golangGoToImplementation",
        "title": "Go to Implementation (Golang)",
        "icon": "$(symbol-method)"
      }
    ],
    "keybindings": [
      {
        "command": "golangGoToImplementation",
        "key": "ctrl+alt+f12",
        "mac": "cmd+alt+f12",
        "when": "editorTextFocus && editorLangId == 'go'"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Golang Go to Implementation",
      "properties": {
        "golangGoToImplementation.excludePatterns": {
          "type": "array",
          "items": {
            "type": "string",
            "example": "**/*_mock.go"
          },
          "default": [
            "**/*_mock.go",
            "**/mock/**/*.go",
            "**/mocks/**/*.go",
            "**/mock_*.go"
          ],
          "description": "List of glob patterns to exclude files from Go to Implementation results."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./out/test/runTest.js",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test:watch": "npm run compile && npm run test",
    "package": "npx @vscode/vsce@2.32.0 package",
    "publish:vscode": "npx @vscode/vsce@2.32.0 publish",
    "publish:openvsx": "npx ovsx publish"
  },
  "dependencies": {
    "minimatch": "^3.1.2"
  },
  "devDependencies": {
    "@types/minimatch": "^5.1.2",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.4",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.4.3",
    "vscode": "^1.1.37"
  }
}